{
  "name": "igniteAndross",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "start": "node node_modules/react-native/local-cli/cli.js start",
    "test": "jest",
    "clean": "rm -rf $TMPDIR/react-* && watchman watch-del-all && npm cache clean --force",
    "clean:android": "cd android/ && ./gradlew clean && cd .. && react-native run-android",
    "newclear": "rm -rf $TMPDIR/react-* && watchman watch-del-all && rm -rf ios/build && rm -rf node_modules/ && npm cache clean --force && npm i",
    "test:watch": "jest --watch",
    "updateSnapshot": "jest --updateSnapshot",
    "coverage": "jest --coverage && open coverage/lcov-report/index.html || xdg-open coverage/lcov-report/index.html",
    "android:build": "cd android && ./gradlew assembleRelease",
    "android:install": "npm run android:uninstall && cd android && ./gradlew assembleRelease && ./gradlew installRelease",
    "android:uninstall": "cd android && adb uninstall com.igniteandross",
    "android:hockeyapp": "$ANDROID_HOME/platform-tools/adb uninstall com.igniteandross",
    "android:devices": "$ANDROID_HOME/platform-tools/adb devices",
    "android:logcat": "$ANDROID_HOME/platform-tools/adb logcat *:S ReactNative:V ReactNativeJS:V",
    "android:shake": "$ANDROID_HOME/platform-tools/adb devices | grep '\\t' | awk '{print $1}' | sed 's/\\s//g' | xargs -I {} $ANDROID_HOME/platform-tools/adb -s {} shell input keyevent 82",
    "storybook": "storybook start -p 7007",
    "lint": "standard --verbose | snazzy",
    "lintdiff": "git diff --name-only --cached --relative | grep '\\.js$' | xargs standard | snazzy",
    "fixcode": "standard --fix",
    "git-hook": "npm run lint -s && npm run test -s",
    "fix-modal": "node node_modules/react-native-modal-translucent/scripts/translucent-modal.js",
    "postinstall": "npx jetify && patch-package && npm run fix-modal"
  },
  "dependencies": {
    "@react-native-community/async-storage": "^1.6.1",
    "@react-native-community/netinfo": "^4.1.3",
    "async": "^3.1.0",
    "base-64": "^0.1.0",
    "bluebird": "^3.5.5",
    "date-fns": "^1.30.1",
    "events": "^3.0.0",
    "faker": "^4.1.0",
    "formik": "^1.5.7",
    "jsc-android": "245459.0.0",
    "lodash": "^4.17.11",
    "pouchdb": "^7.1.1",
    "pouchdb-adapter-asyncstorage": "^6.4.1",
    "pouchdb-adapter-http": "^7.1.1",
    "pouchdb-adapter-react-native-sqlite": "^2.0.0",
    "pouchdb-authentication": "^1.1.3",
    "prop-types": "^15.6.1",
    "react": "16.8.6",
    "react-native": "0.62.3",
    "react-native-action-button": "^2.8.5",
    "react-native-animatable": "^1.3.2",
    "react-native-bottom-action-sheet": "^0.0.23",
    "react-native-calendar-events": "^1.7.3",
    "react-native-dev-menu": "^3.0.0",
    "react-native-device-info": "^2.3.0",
    "react-native-devmenu-trigger": "^1.1.0",
    "react-native-dialogs": "^1.0.5",
    "react-native-document-picker": "^3.2.4",
    "react-native-gesture-handler": "^1.3.0",
    "react-native-immediate-phone-call": "^1.0.0",
    "react-native-in-app-message": "^1.0.2",
    "react-native-keyboard-aware-scroll-view": "^0.9.1",
    "react-native-largelist-v3": "^3.0.14",
    "react-native-linear-gradient": "^2.5.4",
    "react-native-material-bottom-navigation": "^1.0.3",
    "react-native-modal": "^11.3.1",
    "react-native-modal-datetime-picker": "^7.5.0",
    "react-native-modal-translucent": "^3.0.1",
    "react-native-permissions": "^1.2.0",
    "react-native-picker-select": "^6.3.0",
    "react-native-reanimated": "^1.2.0",
    "react-native-screens": "^1.0.0-alpha.23",
    "react-native-slider-custom": "https://github.com/emmanuellmota/react-native-slider",
    "react-native-spring-scrollview": "^2.0.22",
    "react-native-sqlite-2": "^2.0.2",
    "react-native-tab-view": "^2.10.0",
    "react-native-tags": "^2.1.0",
    "react-native-timeline-feed": "^2.0.0",
    "react-native-vector-icons": "^6.6.0",
    "react-navigation": "^3.11.0",
    "react-navigation-collapsible": "^3.0.3",
    "react-navigation-props-mapper": "^1.0.1",
    "react-navigation-redux-helpers": "^3.0.2",
    "react-navigation-transitions": "^1.0.12",
    "react-redux": "^7.1.0",
    "redux": "^4.0.0",
    "redux-devtools-extension": "^2.13.8",
    "redux-persist": "^5.10.0",
    "redux-persist-seamless-immutable": "^2.0.0",
    "redux-persist-transform-filter": "^0.0.18",
    "redux-thunk": "^2.3.0",
    "rn-placeholder": "^3.0.0",
    "rxdb": "^8.3.1",
    "rxdb-utils": "^2.0.0-beta.1",
    "rxjs": "^6.5.2",
    "seamless-immutable": "^7.1.4",
    "shortid": "^2.2.14"
  },
  "devDependencies": {
    "@babel/core": "^7.4.4",
    "@babel/runtime": "^7.4.4",
    "@react-native-community/eslint-config": "^0.0.5",
    "@storybook/addon-storyshots": "^4.1.11",
    "@storybook/addons": "^4.1.11",
    "@storybook/channels": "^4.1.11",
    "@storybook/react-native": "^4.1.11",
    "babel-core": "6.26.3",
    "babel-eslint": "7.1.1",
    "babel-jest": "^24.8.0",
    "babel-plugin-transform-remove-console": "^6.9.4",
    "babel-preset-env": "^1.6.1",
    "babel-preset-react-native": "^4.0.1",
    "enzyme": "^3.2.0",
    "enzyme-adapter-react-16": "^1.1.0",
    "ignite-redux-persist": "1.1.2",
    "ignite-standard": "1.0.0",
    "jest": "^24.8.0",
    "jetifier": "^1.6.3",
    "metro-react-native-babel-preset": "^0.56.0",
    "patch-package": "^6.1.2",
    "postinstall-postinstall": "^2.0.0",
    "react-devtools-core": "3.6.0",
    "react-dom": "16.8.6",
    "react-native-bundle-visualizer": "^1.4.1",
    "react-test-renderer": "16.8.3",
    "redux-logger": "^3.0.6",
    "remote-redux-devtools": "^0.5.16",
    "standard": "10.0.2"
  },
  "jest": {
    "preset": "react-native",
    "testMatch": [
      "<rootDir>/Tests/**/*.js",
      "**/?(*.)(spec|test).js?(x)"
    ],
    "testPathIgnorePatterns": [
      "/node_modules/",
      "<rootDir>/Tests/Setup.js"
    ],
    "moduleNameMapper": {
      "^.+\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "identity-obj-proxy"
    },
    "transform": {
      "^.+\\.(js)$": "<rootDir>/node_modules/react-native/jest/preprocessor.js"
    },
    "setupFiles": [
      "<rootDir>/Tests/Setup"
    ]
  },
  "config": {},
  "standard": {
    "parser": "babel-eslint",
    "globals": [
      "describe",
      "test",
      "jest",
      "expect",
      "fetch",
      "navigator",
      "__DEV__",
      "XMLHttpRequest",
      "FormData",
      "React$Element"
    ]
  }
}
